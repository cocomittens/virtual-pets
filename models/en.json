{
  "invocation": "virtual pet haven",
  "version": "4.0",
  "intents": {
    "YesIntent": {
      "phrases": [
        "yes",
        "yes please",
        "sure"
      ]
    },
    "NoIntent": {
      "phrases": [
        "no",
        "no thanks"
      ]
    },
    "PetTypeIntent": {
      "phrases": [
        "{petType}",
        "{petType} pet",
        "I would like {petType}",
        "I choose {petType}"
      ],
      "entities": {
        "petType": {
          "type": "PetType"
        }
      }
    }
  },
  "entityTypes": {
    "PetType": {
      "values": ["dog", "cat", "bunny"]
    }
  }
}